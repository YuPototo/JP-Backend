{
    "name": "backend_v6",
    "version": "1.0.0",
    "main": "src/index.js",
    "license": "MIT",
    "scripts": {
        "db_up": "docker-compose -f docker-compose.dev.yml -p en_db up -d",
        "db_down": "docker-compose -f docker-compose.dev.yml -p en_db down",
        "build": "tsc",
        "dev": "ENV_FILE=./config/.env.dev tsnd -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
        "start": "ENV_FILE=./config/.env.prod node -r tsconfig-paths/register ./dist/index.js",
        "test": "ENV_FILE=./config/.env.test jest --runInBand"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv-extended": "^2.9.0",
        "dotenv-parse-variables": "^2.0.0",
        "express": "^4.18.1",
        "helmet": "^5.1.1",
        "mongoose": "^6.4.6",
        "morgan": "^1.10.0",
        "morgan-body": "^2.6.6",
        "redis": "^4.2.0",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4",
        "validator": "^13.7.0",
        "winston": "^3.8.1",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/dotenv-parse-variables": "^2.0.1",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/morgan": "^1.9.3",
        "@types/supertest": "^2.0.12",
        "@types/validator": "^13.7.5",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.3",
        "prettier": "^2.7.1",
        "redis-mock": "^0.56.3",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "ts-node-dev": "^2.0.0"
    }
}
